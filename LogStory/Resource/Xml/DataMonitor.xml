<?xml version="1.0" encoding="utf-8" standalone="no"?>
<LSFormatter>
    
    <LSRegex>
        <LSPattern>(?&lt;=\n|\A)\[[0-9a-fA-F]+?\]\[([0-9]{2}[a-zA-Z]{3}[0-9]{2}\s[0-9]{2}:[0-9]{2}:[0-9]{2}\.[0-9]{3})\]\s(?:\[(.+?)\]\s)?(.+?)\n(?=\[|\z)</LSPattern>
        <CaseSensitive>TRUE</CaseSensitive>
        <MatchLine>FALSE</MatchLine>
        
        <LSCapture>
            <Name>Time</Name>
            <Type>String</Type>
        </LSCapture>
        <LSCapture>
            <Name>LogLevel</Name>
            <Type>String</Type>
        </LSCapture>
        <LSCapture>
            <Name>Event</Name>
            <Type>String</Type>
        </LSCapture>
        
    </LSRegex>
    
    <LSTimeFormat>yyyy-MM-dd hh:mm:ss.SSS</LSTimeFormat>
    
</LSFormatter>