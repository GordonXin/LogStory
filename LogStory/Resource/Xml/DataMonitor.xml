<?xml version="1.0" encoding="utf-8" standalone="no"?>
<LSFormatter>
    
    <LSRegex>
        <LSPattern>(?&lt;=\n|\A)\[[0-9a-fA-F]+?\]\[([0-9]{2}[a-zA-Z]{3}[0-9]{2}\s[0-9]{2}:[0-9]{2}:[0-9]{2}\.[0-9]{3})\]\s(?:\[(.+?)\]\s)?(.+?)\n(?=\\[|\\z)</LSPattern>
        <LSCaseSensitive>TRUE</LSCaseSensitive>
        <LSMatchLine>FALSE</LSMatchLine>
        
        <LSCapture>
            <LSName>Time</LSName>
            <LSType>String</LSType>
        </LSCapture>
        <LSCapture>
            <LSName>LogLevel</LSName>
            <LSType>String</LSType>
        </LSCapture>
        <LSCapture>
            <LSName>Event</LSName>
            <LSType>String</LSType>
        </LSCapture>
        
    </LSRegex>
    
    <LSTimeFormat>yyyy-MM-dd hh:mm:ss.SSS</LSTimeFormat>
    
</LSFormatter>